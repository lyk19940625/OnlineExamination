<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="user" catalog="exam" class="entity.User">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="reading" catalog="exam" class="entity.Reading">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="blank" catalog="exam" class="entity.Blank">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="choice" catalog="exam" class="entity.Choice">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="point" catalog="exam" class="entity.Point">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="exam" catalog="exam" class="entity.Exam">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="errquestion" catalog="exam"
		class="entity.ErrorQuestion">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="finish" catalog="exam" class="entity.Finish">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>