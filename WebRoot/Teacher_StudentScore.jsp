					                <td><c:out value="${finish.uname}"/></td>
					           <td><a href="finish/search_finish1?finishid=${finish.finishid}">${finish.examtitle}</a></td>
					                <td><input type="text" value="${finish.totalscore}" style="border:0px; color:red;"/></td>
